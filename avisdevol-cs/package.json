{
  "name": "avisdevol-cs",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:avisdevol-cs": "node dist/avisdevol-cs/server/server.mjs",
    "lint": "ng lint",
    "lint:check-warnings": "ng run avisdevol-cs:lint",
